{
    "name": "trackr",
    "type": "module",
    "private": true,
    "packageManager": "pnpm@10.27.0",
    "scripts": {
        "build": "nuxt build",
        "dev": "cross-env NODE_ENV=development nuxt dev --inspect",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "lint": "eslint .",
        "typecheck": "nuxt typecheck",
        "clean": "rm -rf .nuxt && rm -rf .output && rm -rf .data",
        "clean:all": "pnpm clean && rm -rf node_modules && rm pnpm-lock.yaml",
        "reset": "pnpm clean && pnpm prepare",
        "reset:all": "pnpm clean:all && pnpm install",
        "db:generate": "pnpm exec drizzle-kit generate",
        "db:migrate": "pnpm exec drizzle-kit migrate",
        "db:push": "pnpm exec drizzle-kit push"
    },
    "dependencies": {
        "@iconify-json/lucide": "^1.2.82",
        "@iconify-json/simple-icons": "^1.2.63",
        "@nuxt/ui": "^4.3.0",
        "@nuxtjs/supabase": "2.0.3",
        "@pinia/nuxt": "^0.11.3",
        "@vercel/analytics": "^1.6.1",
        "drizzle-orm": "1.0.0-beta.10-4a43a22",
        "lodash-es": "^4.17.22",
        "nuxt": "^4.2.2",
        "nuxt-charts": "^2.0.0",
        "nuxt-ssr-api-logger": "2.0.2",
        "pinia": "^3.0.4",
        "postgres": "^3.4.8",
        "zod": "^4.3.5"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^6.7.3",
        "@nuxt/eslint": "^1.12.1",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^25.0.3",
        "@types/pg": "^8.16.0",
        "cross-env": "^10.1.0",
        "drizzle-kit": "1.0.0-beta.10-4a43a22",
        "eslint": "^9.39.2",
        "eslint-plugin-format": "^1.1.0",
        "typescript": "^5.9.3",
        "vue-tsc": "^3.2.1"
    }
}
